@using MMi_BIS_PA.Models;

@{
    var Barcode = ViewBag.Barcode;
}
<div class="container-fluid">

    <div class="row" style="width:100%;height:30%">

    </div>
    <div class="row" style="height:20%;">
        <div class="col">
            @if (Barcode.Data == "same")
            {
                <h1 class="text-danger">Same Barcode Scanned</h1>
            }
            else
            {
                <h1>@Barcode.Data</h1>
            }
        </div>



        <div class="col-2"></div>
    </div>
    <div class="row" style="width:100%;height:30%"></div>

</div>




